#include <iostream>
using namespace std;

typedef x1
{
	int x;
} Tx1, *PTx1;

int strcmp (char* s1, char* s2, int n)
{
	int i = 0;
	while (((s1[i] - s2[i]) == 0) && (s1[i] != '\0') && (i < n -1))
	{
	  i++;
	}

	if ((s1[i] - s2[i]) > 0)
		return 1;
	else if (( s1[i] - s2[i]) < 0)
		   return -1;
		else return 0;
}


// {{{1,2},{3,4}}, {{5,6},{7,8}}}
int main()
{
	char s1[200] = "aaab";
	char s2[200] = "aaazasd";
	cout << strcmp (s1, s2, 3);
}
