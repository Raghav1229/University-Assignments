#ifndef SPECTER2HTML_HPP_
#define SPECTER2HTML_HPP_

#ifndef string
#include <string>
#endif

class Specter2HTML
{
public:
	string specter2String (int* specter, int axes, int maxF,int length, int nonZeroElementCounter);
	string specter2COut (int* specter, int axes, int maxF,int length, int nonZeroElementCounter);
	string specter2File (int* specter, int axes, int maxF,int length, int nonZeroElementCounter);
}



#endif /* SPECTER2HTML_HPP_ */
